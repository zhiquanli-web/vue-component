    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify/json": "^2.1.123",
    "@types/mockjs": "^1.0.7",
    "@types/node": "18.7.18",
    "@types/nprogress": "^0.2.0",
