    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "typescript": "^4.6.4",
    "unocss": "^0.45.30",
    "unplugin-auto-import": "^0.11.3",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.0",
    "vite-plugin-mock": "^2.9.6",
    "vue-tsc": "^0.40.4"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
